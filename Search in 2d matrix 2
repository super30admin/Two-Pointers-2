
class Solution(object):
    def searchMatrix(self, matrix, target):
        """
        :type matrix: List[List[int]]
        :type target: int
        :rtype: bool
        """
        rows=len(matrix)
        col=len(matrix[0])
        i=0
        j=col-1
        while(i<rows and j>=0):
            if(matrix[i][j]==target):
                return True
            elif(matrix[i][j]<target):
                i=i+1
            else:
                j=j-1
        return False
