class Solution {
    public int removeDuplicates(int[] nums) 
    {
        if (nums == null)
        {
            return 0;
        } 
            int slow = 1;
            int count =1;
            
            for(int fast=1 ; fast<nums.length;fast++)
             {
                 if(nums[fast-1] == nums[fast])
                 {
                     count++;
                 }
                 
                 else
                 {
                     count = 1;
                 }
                 if(count <= 2)
                 {
                     nums[slow] = nums[fast];
                     slow++;
                     
                 }
             }
               
        return slow;
    }
}

//Time : O(n)
//Space : O(1)
